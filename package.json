		"babel-jest": "^26.6.3",
		"core-js": "^3.8.3",
		"eslint": "^7.19.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"rollup": "^2.38.5",
		"rollup-plugin-filesize": "^9.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"vue": "^2.6.12",
