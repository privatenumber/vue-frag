{
	"name": "vue-frag",
	"version": "0.0.0-semantic-release",
	"description": "Vue 2 fragment directive to return multiple root elements",
	"keywords": [
		"vue",
		"fragment",
		"directive",
		"ssr",
		"multiple root nodes"
	],
	"license": "MIT",
	"repository": "privatenumber/vue-frag",
	"funding": "https://github.com/privatenumber/vue-frag?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "dist/frag.cjs.js",
	"module": "dist/frag.esm.js",
	"unpkg": "dist/frag.js",
	"types": "./frag.d.ts",
	"scripts": {
		"build": "rollup -c --environment NODE_ENV:production",
		"test": "jest",
		"lint": "eslint ."
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix",
			"jest --bail --findRelatedTests"
		]
	},
	"devDependencies": {
		"@babel/preset-env": "^7.15.6",
		"@babel/preset-typescript": "^7.15.0",
		"@pvtnbr/eslint-config": "^0.2.1",
		"@rollup/plugin-babel": "^5.3.0",
		"@types/jest": "^27.0.2",
		"@vue/composition-api": "^1.2.2",
		"@vue/server-test-utils": "^1.1.4",
		"@vue/test-utils": "^1.1.4",
		"es-jest": "^1.3.0",
		"eslint": "^7.19.0",
		"husky": "^4.3.8",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"outdent": "^0.8.0",
		"rollup": "^2.57.0",
		"rollup-plugin-filesize": "^9.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"typescript": "^4.4.3",
		"vue": "^2.6.12",
		"vue-server-renderer": "^2.6.12",
		"vue-template-compiler": "^2.6.12"
	},
	"eslintConfig": {
		"extends": "@pvtnbr",
		"env": {
			"browser": true
		},
		"rules": {
			"symbol-description": "off",
			"unicorn/no-array-for-each": "off"
		}
	}
}
